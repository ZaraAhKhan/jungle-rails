<tr>
  <td>
  <%@product = Product.find product%>
    <%= image_tag @product.image.tiny %>
  </td>
  <td>
    <h4><%= @product.name %></h4>
    <br>
    <p><%= @product.description %></p>
    <br>
    </td>
  <td>
  <p><%= humanized_money_with_symbol @product.price_cents/100 %></p>
  </td>
  <td>
    <p><%= quantity  %></p>
  </td>
  <td>
    <p><%= humanized_money_with_symbol (quantity * @product.price_cents)/100 %></p>
  </td>
  
  
</tr>